{
  "name": "gd-bs",
  "version": "2.1.5",
  "description": "Bootstrap JavaScript, TypeScript and Web Components library.",
  "main": "dist/gd-bs.js",
  "typings": "dist/gd-bs.d.ts",
  "scripts": {
    "all": "npm run icons && npm run build-all && npm run prod-all && npm run typings",
    "build-all": "npm run build && npm run build-icons",
    "build": "webpack --mode=development",
    "build-icons": "webpack --mode=development --icons=true",
    "icons": "node generateIcons",
    "prod-all": "npm run prod && npm run prod-icons",
    "prod": "webpack --mode=production",
    "prod-icons": "webpack --mode=production --icons=true",
    "typings": "dts-bundle --out ../dist/gd-bs.d.ts --name gd-bs --main ./@types/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/gd-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "Web Components"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/gd-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-icons": "^1.0.0-alpha3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/node": "^13.13.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "dts-bundle": "^0.7.3",
    "html-loader": "^1.1.0",
    "jquery": "^3.5.0",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^7.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
