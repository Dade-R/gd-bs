{
  "name": "gd-bs",
  "version": "4.0.6",
  "description": "Bootstrap JavaScript, TypeScript and Web Components library.",
  "main": "dist/gd-bs-icons.min.js",
  "typings": "@types/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run pre-build && npm run build && npm run prod",
    "build": "npm run build-icons && npm run build-no-icons && npm run build-core",
    "build-core": "webpack --mode=development --config=webpack.core.js",
    "build-icons": "webpack --mode=development --config=webpack.icons.js",
    "build-no-icons": "webpack --mode=development",
    "clean": "node ./clean.js",
    "pre-build": "npm run pre-build-icons && tsc && npm run pre-build-sass",
    "pre-build-icons": "node ./generate && node ./generateIcons",
    "pre-build-sass": "webpack --config webpack.sass.js",
    "prod": "npm run prod-core && npm run prod-no-icons && npm run prod-icons",
    "prod-core": "webpack --mode=production --config=webpack.core.js",
    "prod-icons": "webpack --mode=production --config=webpack.icons.js",
    "prod-no-icons": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/gd-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "TypeScript"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/gd-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta2",
    "bootstrap-icons": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/node": "^14.14.16",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "html-loader": "^1.3.2",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  }
}