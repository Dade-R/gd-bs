{
  "name": "gd-bs",
  "version": "3.1.8",
  "description": "Bootstrap JavaScript, TypeScript and Web Components library.",
  "main": "build/index.js",
  "typings": "@types/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run pre-build && npm run build && npm run prod",
    "build": "npm run build-core && npm run build-no-icons && npm run build-icons",
    "build-core": "webpack --mode=development --core=true",
    "build-icons": "webpack --mode=development --icons=true",
    "build-no-icons": "webpack --mode=development",
    "clean": "node ./clean.js",
    "pre-build": "npm run pre-build-icons && tsc && npm run pre-build-sass",
    "pre-build-icons": "node ./generate && node ./generateIcons",
    "pre-build-sass": "webpack --config webpack.sass.js",
    "prod": "npm run prod-core && npm run prod-no-icons && npm run prod-icons",
    "prod-core": "webpack --mode=production --core=true",
    "prod-icons": "webpack --mode=production --icons=true",
    "prod-no-icons": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/gd-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "TypeScript"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/gd-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/node": "^14.0.26",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "bootstrap-icons": "^1.0.0-alpha5",
    "css-loader": "^3.6.0",
    "html-loader": "^1.1.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  }
}
