{
  "name": "gd-bs",
  "version": "2.4.7",
  "description": "Bootstrap JavaScript, TypeScript and Web Components library.",
  "main": "build/index-icons.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run libs && npm run build-all && npm run prod-all",
    "build-all": "npm run build && npm run build-core && npm run build-icons",
    "build": "npm run build-js && webpack --mode=development",
    "build-core": "webpack --mode=development --core=true",
    "build-icons": "webpack --mode=development --icons=true",
    "build-js": "tsc && copyfiles -u 1 \"src/**/*.html\" \"src/sass/*\" ./build",
    "clean": "node ./clean.js",
    "libs": "node ./generate && node ./generateIcons",
    "prod-all": "npm run prod && npm run prod-core && npm run prod-icons",
    "prod": "webpack --mode=production",
    "prod-core": "webpack --mode=production --core=true",
    "prod-icons": "webpack --mode=production --icons=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/gd-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "Web Components"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://dattabase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/gd-bs/issues"
  },
  "homepage": "https://dattabase.com/extras/bs",
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-icons": "^1.0.0-alpha3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/node": "^14.0.1",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.3.0",
    "css-loader": "^3.5.3",
    "html-loader": "^1.1.0",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^7.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
