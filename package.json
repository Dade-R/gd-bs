{
  "name": "gd-bs",
  "version": "0.7.8",
  "description": "Bootstrap JavaScript, TypeScript and Web Components library.",
  "main": "dist/gd-bs.js",
  "typings": "src/typings.d.ts",
  "scripts": {
    "all": "npm run build && npm run prod && npm run typings && npm run wc",
    "build": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "typings": "dts-bundle --out ../dist/gd-bs.d.ts --name gd-bs --main ./src/typings.d.ts",
    "wc": "cd wc && npm run build",
    "wc-serve": "cd wc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunjandatta/gd-bs.git"
  },
  "keywords": [
    "Bootstrap",
    "Web Components"
  ],
  "author": "Gunjan Datta <me@dattabase.com> (https://gunjandatta.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunjandatta/gd-bs/issues"
  },
  "homepage": "https://gunjandatta.github.io/extras/bs",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "dts-bundle": "^0.7.3",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.3",
    "popper.js": "^1.14.4",
    "postcss-loader": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
